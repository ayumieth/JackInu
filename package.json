{
  "name": "fastswap-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build-test": "env-cmd -f ./.env.development react-scripts build",
    "build-mainnet": "env-cmd -f ./.env.production react-scripts build",
    "start-mainnet": "env-cmd -f ./.env.production react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "generate:config": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/generateConfig",
    "deploy": "yarn build-test && mv build/index.html build/200.html && surge -p build -d fastswap.surge.sh",
    "deploy:vercel": "vercel --prod"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@crowdin/crowdin-api-client": "^1.10.4",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@ethersproject/abi": "^5.0.7",
    "@jackinu-uikit": "https://github.com/ayumieth/jackinu-uikit.git",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.1.9",
    "apexcharts": "^3.35.4",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "chart.js": "^3.9.1",
    "date-fns": "^2.17.0",
    "ethers": "^5.0.31",
    "html-loader": "^4.1.0",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "lottie-react": "^2.3.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-apexcharts": "^1.4.0",
    "react-countdown": "^2.3.2",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.8",
    "react-icons": "^4.4.0",
    "react-particles": "2.2.1",
    "react-redux": "^7.2.2",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-scroll": "^1.8.7",
    "react-use": "^17.3.2",
    "styled-components": "^5.1.1",
    "tsparticles": "^2.2.2",
    "tsparticles-engine": "^2.2.2",
    "typescript": "^4.1.5",
    "web3": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "1.0.0",
    "@types/jest": "^26.0.19",
    "@types/react-redux": "^7.1.16",
    "env-cmd": "^10.1.0",
    "eslint": "^7.20.0",
    "husky": "4.3.5",
    "prettier": "^2.1.2",
    "source-map-explorer": "^2.5.2",
    "ts-node": "^9.1.1"
  }
}
